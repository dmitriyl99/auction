{
    "name": "skaydi/auction",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "^8.0",
        "php-di/php-di": "^6.3",
        "psr/container": "1.1.1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "symfony/console": "^5.3"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "skaydi",
            "email": "dm1triyl99@yandex.com"
        }
    ],
    "require-dev": {
        "overtrue/phplint": "^3.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "lint": "phplint",
        "psalm": "psalm"
    }
